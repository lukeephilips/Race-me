<%= form_for [@user, run] do |f| %>
    <%= f.label :goal_id %>
    <%= f.select(:goal_id, @user.goals.collect{|goal| [goal.name, goal.id]}) %>
    <%= f.submit 'Assign Run', class: 'btn' %>
<% end %>

